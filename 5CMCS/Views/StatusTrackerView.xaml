<UserControl x:Class="_5CMCS.Views.StatusTrackerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel>
        <TextBlock Text="Claim Status Tracker" Style="{StaticResource TitleText}"/>
        <Border Style="{StaticResource Card}" Margin="0,12,0,0">
            <StackPanel Margin="150">
                <TextBlock Text="None" FontStyle="Italic" Foreground="{StaticResource Ink500}" />
            </StackPanel>
        </Border>
    </StackPanel>
</UserControl>